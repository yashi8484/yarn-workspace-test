{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":timezone(Asia/Tokyo)",
    ":prHourlyLimitNone",
    ":maintainLockFilesWeekly",
    ":automergeTypes",
    "presets/test.json"
  ],
  "enabledManagers": ["npm"],
  "labels": ["dependencies"],
  "prConcurrentLimit": 16,
  "schedule": [
    "* */1 * * *"
  ],
  "automergeSchedule": [
    "* */1 * * *"
  ],
  "ignoreTests": true,
  "rangeStrategy": "update-lockfile"
}
